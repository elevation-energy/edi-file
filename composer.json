{
    "name": "elevation-energy/edi-file",
    "description": "Contains classes for building EDI file objects and casting to string",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Tallant",
            "email": "justin.tallant@elevationeg.com"
        }
    ],
    "require": {
        "php": "~7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0@dev",
        "phpunit/phpunit": "~8.1",
        "symfony/var-dumper": "^4.3@dev"
    },
    "autoload": {
        "psr-4": {
            "Elevation\\EDIFile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elevation\\EDIFile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "coverage": "./vendor/bin/phpunit --coverage-html build/coverage"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.17"
        }
    },
    "minimum-stability": "dev"
}
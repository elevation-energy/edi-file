{
    "name": "elevation-energy/edi-file",
    "description": "Contains classes for building EDI file objects and casting to string",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Tallant",
            "email": "justin.tallant@elevationeg.com"
        },
	{
            "name": "Nick Mendoza",
            "email": "nick@carbonbetter.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.10",
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "Elevation\\EDIFile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elevation\\EDIFile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "coverage": "./vendor/bin/phpunit --coverage-html build/coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
